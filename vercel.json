{
  "version": 2,
  "builds": [
      {
          "src": "build/elearning-server/index.js",
          "use": "@vercel/node",
          "config": { "includeFiles": ["dist/**"] }
      }
  ],
  "routes": [
      {
          "src": "/(.*)",
          "dest": "build/elearning-server/index.js"
      }
  ]
}